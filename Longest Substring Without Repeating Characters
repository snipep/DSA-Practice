T.C ==> O(N)
S.P ==> O(N)

class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        len1 = 0
        l , r = 0,0
        check = {}
        for i in range(len(s)):
            if s[r] not in check:
                check[s[r]] = r
                r += 1
            else:
                l = max(l, check[s[r]] + 1)
                check[s[r]] = r
                r += 1
                
            len1 = max(len1,(r-l))
        return len1
