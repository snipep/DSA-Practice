class Solution:
    def maximalNetworkRank(self, n, roads):
        adj = [[] for _ in range(n)]
        for u,v in roads:
            adj[u].append(v)
            adj[v].append(u)

        max_rank = 0
        for i in range(n):
            for j in range(i+1, n):
                rank = len(adj[i]) + len(adj[j])
                if i in adj[j] or j in adj[i]:
                    rank -= 1
                
                max_rank = max(rank, max_rank)
        return max_rank
